{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Cake",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceRoot}/tools/Addins/Cake.CoreCLR.0.35.0/Cake.dll",
      "args": ["${workspaceRoot}/build.cake", "-debug", "-target=All"],
      "cwd": "${workspaceRoot}",
      "stopAtEntry": true,
      "externalConsole": false,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
