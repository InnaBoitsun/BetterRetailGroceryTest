<?xml version="1.0"?>
<!--  
:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
Full documentation for CTT transforms http://msdn.microsoft.com/en-us/library/dd465326.aspx 
:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
-->
<settings xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <add key="OrchestrationServicesHostname" value="{ocs-cm-hostName}" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
  <add key="Overture.Url" value="https://{ocs-cm-hostName}/api/" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
  <add key="Overture.AuthToken" value="{ocsAuthToken}" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
</settings>